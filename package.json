{
  "name": "arch-ui-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run lint:fix && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:pwa": "npx http-server -p 8080",
    "build:pwa": "ng build --configuration=pwa",
    "pwa:https": "ng serve --configuration=pwa --ssl true",
    "pwa": "ng serve --configuration=pwa",
    "test": "ng test",
    "test:watch:coverage": "shx rm -rf coverage && ng test --watch=true --source-map=true --browsers=ChromeHeadlessNoSandbox --code-coverage=true",
    "lint": "ng lint --max-warnings 0",
    "lint:fix": "ng lint --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.6",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.6",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/service-worker": "^20.1.0",
    "bootstrap": "^5.3.8",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/schematics": "^20.1.1",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-eslint": "^20.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "propeteer": "^0.0.5",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.40.0"
  }
}
